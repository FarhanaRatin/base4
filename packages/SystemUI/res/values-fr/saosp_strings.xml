<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Quick settings -->
    <string name="quick_settings_brightness_label">Luminosité</string>
    <string name="quick_settings_heads_up_label">Notifications flottantes</string>
    <string name="quick_settings_screen_off_label">Éteindre l\'écran</string>
    <string name="quick_settings_screenshot_label">Capture d\'écran</string>
    <string name="quick_settings_volume_panel_label">Panneau de volume</string>

    <!-- Content description of the brightness tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_brightness">Luminosité.</string>

    <!-- Content description of the screen off tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_screen_off">Éteindre l\'écran.</string>

    <!-- Content description of the screenshot tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_screenshot">Capture d\'écran.</string>

    <!-- Content description of the volume panel tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_volume_panel">Panneau de volume.</string>

    <!-- Content description of the heads up tile in quick settings when off (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_off">Notifications flottantes désactivées.</string>
    <!-- Content description of the heads up tile in quick settings when on (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_on">Notifications flottantes activés.</string>

    <!-- Announcement made when heads up changes to off (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_changed_off">Les notifications flottantes sont désactivées.</string>
    <!-- Announcement made when heads up changes to on (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_changed_on">Les notifications flottantes sont activées.</string>

    <!-- The text for the button in the notification window-shade that clears
         all of the currently visible notifications. [CHAR LIMIT=10]-->
    <string name="clear_recents">Fermer tout les récents</string>

    <!-- Weather string format in expanded statusbar header -->
    <string name="status_bar_expanded_header_weather_format"><xliff:g id="temp">%1$s</xliff:g> - <xliff:g id="condition">%2$s</xliff:g></string>

    <!-- Screen timeout tile -->
    <string name="quick_settings_timeout_label">Mise en veille</string>
    <string name="quick_settings_screen_timeout_detail_title">Mise en veille de l\'écran</string>

    <!-- Content description of the USB tethering tile in quick settings when off (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_usb_tether_off">Modem USB éteint.</string>
    <!-- Content description of the USB tethering tile in quick settings when on (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_usb_tether_on">Modem USB allumé.</string>

    <!-- Announcement made when USB tethering changes to off (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_usb_tether_changed_off">Modem USB est éteint.</string>
    <!-- Announcement made when USB tethering changes to on (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_usb_tether_changed_on">Modem USB est allumée.</string>

    <!-- USB tethering tile -->
    <string name="quick_settings_usb_tether_label">Modem USB</string>  
  
    <!-- Text to use when the number in a notification info is too large
         (greater than status_bar_notification_info_maxnum, defined in
         values/config.xml) and must be truncated.
         [CHAR LIMIT=4] -->
    <string name="status_bar_notification_info_overflow" translatable="false">\u221E</string>
</resources>
